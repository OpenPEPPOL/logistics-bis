<?xml version="1.0" encoding="UTF-8"?>
<!--BII2-T19-R012-->
<testSet xmlns="http://difi.no/xsd/vefa/validator/1.0"
	configuration="peppolbis-T129-base-1.0">
	<assert>
		<description>Verify that the Receiver Party's Name or Identifier is specified. </description>
		<scope>PEPPOL-T129-R032</scope>
	</assert>
	
	<test id="1">
		<assert>
			<success>PEPPOL-T129-R032</success>
		</assert>
		<ubl:ApplicationResponse xmlns:ubl="urn:oasis:names:specification:ubl:schema:xsd:ApplicationResponse-2"
			xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
			xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2">

			<cac:ReceiverParty>
				<cac:PartyIdentification>
					<cbc:ID schemeID="0088">5790000435951</cbc:ID>
				</cac:PartyIdentification>
				<cac:PartyName>
					<cbc:Name>ABC Supplier Ltd.</cbc:Name>
				</cac:PartyName>
			</cac:ReceiverParty>
		</ubl:ApplicationResponse>
	</test>

	<test id="2">
		<assert>
			<success>PEPPOL-T129-R032</success>
		</assert>
		<ubl:ApplicationResponse xmlns:ubl="urn:oasis:names:specification:ubl:schema:xsd:ApplicationResponse-2"
			xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
			xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2">

			<cac:ReceiverParty>
				<cac:PartyName>
					<cbc:Name>ABC Supplier Ltd.</cbc:Name>
				</cac:PartyName>
			</cac:ReceiverParty>
		</ubl:ApplicationResponse>
	</test>
	
	<test id="3">
		<assert>
			<success>PEPPOL-T129-R032</success>
		</assert>
		<ubl:ApplicationResponse xmlns:ubl="urn:oasis:names:specification:ubl:schema:xsd:ApplicationResponse-2"
			xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
			xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2">

			<cac:ReceiverParty>
				<cac:PartyIdentification>
					<cbc:ID schemeID="0088">5790000435951</cbc:ID>
				</cac:PartyIdentification>
			</cac:ReceiverParty>
		</ubl:ApplicationResponse>
	</test>
	
	<test id="4">
		<assert>
			<error>PEPPOL-T129-R032</error>
		</assert>
		<ubl:ApplicationResponse xmlns:ubl="urn:oasis:names:specification:ubl:schema:xsd:ApplicationResponse-2"
			xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
			xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2">

			<cac:ReceiverParty>
			</cac:ReceiverParty>
		</ubl:ApplicationResponse>
	</test>

</testSet>