<?xml version="1.0" encoding="UTF-8"?>
<testSet xmlns="http://difi.no/xsd/vefa/validator/1.0" configuration="peppolbis-T120-base-1.1">
	<assert>
		<description>Verify DespatchAdvice Total transport handling unit quantity SHALL not be negative</description>
		<scope>PEPPOL-T120-R016</scope>
	</assert>
	
	<test id="1">
		<assert>
			<success>PEPPOL-T120-R016</success>
		</assert>
		<DespatchAdvice xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
			xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2"
			xmlns="urn:oasis:names:specification:ubl:schema:xsd:DespatchAdvice-2">
			<cac:Shipment>
				<cbc:ID>123456</cbc:ID>
				<cbc:TotalTransportHandlingUnitQuantity>1</cbc:TotalTransportHandlingUnitQuantity>
			</cac:Shipment>
		</DespatchAdvice>
	</test>
	
	<test id="2">
		<assert>
			<success>PEPPOL-T120-R016</success>
		</assert>
		<DespatchAdvice xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
			xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2"
			xmlns="urn:oasis:names:specification:ubl:schema:xsd:DespatchAdvice-2">
			<cac:Shipment>
				<cbc:ID>123456</cbc:ID>
				<cbc:TotalTransportHandlingUnitQuantity>0</cbc:TotalTransportHandlingUnitQuantity>
			</cac:Shipment>
		</DespatchAdvice>
	</test>
	
	<test id="3">
		<assert>
			<success>PEPPOL-T120-R016</success>
		</assert>
		<DespatchAdvice xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
			xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2"
			xmlns="urn:oasis:names:specification:ubl:schema:xsd:DespatchAdvice-2">
			<cac:Shipment>
				<cbc:ID>123456</cbc:ID>
			</cac:Shipment>
		</DespatchAdvice>
	</test>
	
	
	<test id="164">
		<assert>
			<error>PEPPOL-T120-R016</error>
		</assert>
		<DespatchAdvice xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
			xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2"
			xmlns="urn:oasis:names:specification:ubl:schema:xsd:DespatchAdvice-2">
			<cbc:ID>12345678</cbc:ID>
			<cbc:IssueDate>2022-12-24</cbc:IssueDate>
			<cac:DespatchSupplierParty>
				<cac:Party>
					<cbc:EndpointID schemeID="0007">5560125220</cbc:EndpointID>
					<cac:PartyIdentification>
						<cbc:ID schemeID="0007">5560125220</cbc:ID>
					</cac:PartyIdentification>
				</cac:Party>
			</cac:DespatchSupplierParty>
			<cac:DeliveryCustomerParty>
				<cac:Party>
					<cbc:EndpointID schemeID="0007">5560339086</cbc:EndpointID>
					<cac:PartyIdentification>
						<cbc:ID schemeID="0007">5560339086</cbc:ID>
					</cac:PartyIdentification>
					<cac:PartyLegalEntity>
						<cbc:RegistrationName>Skanska Sverige AB</cbc:RegistrationName>
					</cac:PartyLegalEntity>
				</cac:Party>
			</cac:DeliveryCustomerParty>
			<cac:Shipment>
				<cbc:ID>A123</cbc:ID>
				<cbc:TotalTransportHandlingUnitQuantity>-1</cbc:TotalTransportHandlingUnitQuantity>
			</cac:Shipment>
		</DespatchAdvice>
	</test>
</testSet>
