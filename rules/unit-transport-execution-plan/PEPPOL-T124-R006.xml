<?xml version="1.0" encoding="UTF-8"?>
<testSet xmlns="http://difi.no/xsd/vefa/validator/1.0"
	configuration="peppolbis-T124-base-1.0">
	<assert>
		<description>Verify that the gross volumens match the gross volumen on the transport handling units</description>
		<scope>PEPPOL-T124-R006</scope>
	</assert>
	<test>
		<assert>
			<success>PEPPOL-T124-R006:a</success>
		</assert>
		<ubl:TransportExecutionPlan xmlns:ubl="urn:oasis:names:specification:ubl:schema:xsd:TransportExecutionPlan-2"
                            xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
                            xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2">      
		   <cac:Consignment>
		   <!--Gross volumen specified and specified as well on THU level -->      
			  <cbc:GrossVolumeMeasure unitCode="MTQ">20</cbc:GrossVolumeMeasure>
			  <cac:TransportHandlingUnit>
				<cbc:ID>11111222222222</cbc:ID>
				<cac:MeasurementDimension>
					<cbc:AttributeID>AAW</cbc:AttributeID>
					<cbc:Measure unitCode="MTQ">20</cbc:Measure>      
				 </cac:MeasurementDimension>         
			  </cac:TransportHandlingUnit>
		   </cac:Consignment>
		</ubl:TransportExecutionPlan>
	</test>
		
	<test>
		<assert>
			<success>PEPPOL-T124-R006:b</success>
		</assert>
		<ubl:TransportExecutionPlan xmlns:ubl="urn:oasis:names:specification:ubl:schema:xsd:TransportExecutionPlan-2"
                            xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
                            xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2">      
		   <cac:Consignment>
		   <!--Gross volumen specified and but not on THU level -->      
			  <cbc:GrossVolumeMeasure unitCode="MTQ">20</cbc:GrossVolumeMeasure>
			 <cac:TransportHandlingUnit>
				<cbc:ID>11111222222222</cbc:ID>				      
			  </cac:TransportHandlingUnit>
		   </cac:Consignment>
		</ubl:TransportExecutionPlan>
	</test>
	
	<test>
		<assert>
			<success>PEPPOL-T124-R006:c</success>
		</assert>
		<ubl:TransportExecutionPlan xmlns:ubl="urn:oasis:names:specification:ubl:schema:xsd:TransportExecutionPlan-2"
                            xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
                            xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2">      
		   <cac:Consignment>
		   <!--Gross weight specified and specified as well on THU level, can be greater -->      
			  <cbc:GrossVolumeMeasure unitCode="MTQ">25</cbc:GrossVolumeMeasure>
			  <cac:TransportHandlingUnit>
				<cbc:ID>11111222222222</cbc:ID>
				<cac:MeasurementDimension>
					<cbc:AttributeID>AAW</cbc:AttributeID>
					<cbc:Measure unitCode="MTQ">10</cbc:Measure>      
				 </cac:MeasurementDimension>         
			  </cac:TransportHandlingUnit>
			  <cac:TransportHandlingUnit>
				<cbc:ID>11111222222222</cbc:ID>
				<cac:MeasurementDimension>
					<cbc:AttributeID>AAW</cbc:AttributeID>
					<cbc:Measure unitCode="MTQ">10</cbc:Measure>      
				 </cac:MeasurementDimension>         
			  </cac:TransportHandlingUnit>
		   </cac:Consignment>
		</ubl:TransportExecutionPlan>
	</test>
	
	<test>
		<assert>
			<warning>PEPPOL-T124-R006:d</warning>
		</assert>
		<ubl:TransportExecutionPlan xmlns:ubl="urn:oasis:names:specification:ubl:schema:xsd:TransportExecutionPlan-2"
                            xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
                            xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2">      
		   <cac:Consignment>
		   <!--Gross weight specified and specified as well on THU level, can be greater -->      
			  <cbc:GrossVolumeMeasure unitCode="MTQ">15</cbc:GrossVolumeMeasure>
			  <cac:TransportHandlingUnit>
				<cbc:ID>11111222222222</cbc:ID>
				<cac:MeasurementDimension>
					<cbc:AttributeID>AAW</cbc:AttributeID>
					<cbc:Measure unitCode="MTQ">10</cbc:Measure>      
				 </cac:MeasurementDimension>         
			  </cac:TransportHandlingUnit>
			  <cac:TransportHandlingUnit>
				<cbc:ID>11111222222222</cbc:ID>
				<cac:MeasurementDimension>
					<cbc:AttributeID>AAW</cbc:AttributeID>
					<cbc:Measure unitCode="MTQ">10</cbc:Measure>      
				 </cac:MeasurementDimension>         
			  </cac:TransportHandlingUnit>
		   </cac:Consignment>
		</ubl:TransportExecutionPlan>
	</test>

</testSet>
