<?xml version="1.0" encoding="UTF-8"?>
<testSet xmlns="http://difi.no/xsd/vefa/validator/1.0"
	configuration="peppolbis-T124-base-1.0">
	<assert>
		<description>Verify that the grossweight on consignment match the sum of the grossweight on the THU if they are specified</description>
		<scope>PEPPOL-T124-R005</scope>
	</assert>
	<test>
		<assert>
			<success>PEPPOL-T124-R005:a</success>
		</assert>
		<ubl:TransportExecutionPlan xmlns:ubl="urn:oasis:names:specification:ubl:schema:xsd:TransportExecutionPlan-2"
                            xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
                            xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2">      
		   <cac:Consignment>
		   <!--Gross weight specified and specified as the  on THU level -->      
			  <cbc:GrossWeightMeasure unitCode="KGM">1030</cbc:GrossWeightMeasure>
			  <cac:TransportHandlingUnit>
				<cbc:ID>11111222222222</cbc:ID>
				<cac:MeasurementDimension>
					<cbc:AttributeID>AAB</cbc:AttributeID>
					<cbc:Measure unitCode="KGM">1030</cbc:Measure>      
				 </cac:MeasurementDimension>         
			  </cac:TransportHandlingUnit>
		   </cac:Consignment>
		</ubl:TransportExecutionPlan>
	</test>
	
	<test>
		<assert>
			<success>PEPPOL-T124-R005:b</success>
		</assert>
		<ubl:TransportExecutionPlan xmlns:ubl="urn:oasis:names:specification:ubl:schema:xsd:TransportExecutionPlan-2"
                            xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
                            xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2">      
		   <cac:Consignment>
		   <!--Gross weight specified and not specified on THU level -->      
			  <cbc:GrossWeightMeasure unitCode="KGM">1030</cbc:GrossWeightMeasure>
			  <cac:TransportHandlingUnit>
				<cbc:ID>11111222222222</cbc:ID>				      
			  </cac:TransportHandlingUnit>
		   </cac:Consignment>
		</ubl:TransportExecutionPlan>
	</test>
	
	<test>
		<assert>
			<success>PEPPOL-T124-R005:c</success>
		</assert>
		<ubl:TransportExecutionPlan xmlns:ubl="urn:oasis:names:specification:ubl:schema:xsd:TransportExecutionPlan-2"
                            xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
                            xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2">      
		   <cac:Consignment>
		   <!--Gross weight specified and specified as the  on THU level -->      
			  <cbc:GrossWeightMeasure unitCode="KGM">2030</cbc:GrossWeightMeasure>
			  <cac:TransportHandlingUnit>
				<cbc:ID>11111222222222</cbc:ID>
				<cac:MeasurementDimension>
					<cbc:AttributeID>AAB</cbc:AttributeID>
					<cbc:Measure unitCode="KGM">1030</cbc:Measure>      
				 </cac:MeasurementDimension>         
			  </cac:TransportHandlingUnit>
			  <cac:TransportHandlingUnit>
				<cbc:ID>11111222222223</cbc:ID>
				<cac:MeasurementDimension>
					<cbc:AttributeID>AAB</cbc:AttributeID>
					<cbc:Measure unitCode="KGM">1000</cbc:Measure>      
				 </cac:MeasurementDimension>         
			  </cac:TransportHandlingUnit>
		   </cac:Consignment>
		</ubl:TransportExecutionPlan>
	</test>
	
		
	<test>
		<assert>
			<warning>PEPPOL-T124-R005:d</warning>
		</assert>
		<ubl:TransportExecutionPlan xmlns:ubl="urn:oasis:names:specification:ubl:schema:xsd:TransportExecutionPlan-2"
                            xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
                            xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2">      
		   <cac:Consignment>      
			  <cbc:GrossWeightMeasure unitCode="KGM">1030</cbc:GrossWeightMeasure>
			  <cac:TransportHandlingUnit>
				<cbc:ID>11111222222222</cbc:ID>
				<cac:MeasurementDimension>
					<cbc:AttributeID>AAB</cbc:AttributeID>
					<cbc:Measure unitCode="KGM">1000</cbc:Measure>      
				 </cac:MeasurementDimension>         
			  </cac:TransportHandlingUnit>			   
		   </cac:Consignment>
		</ubl:TransportExecutionPlan>
	</test>

	<test>
		<assert>
			<warning>PEPPOL-T124-R005:e</warning>
		</assert>
		<ubl:TransportExecutionPlan xmlns:ubl="urn:oasis:names:specification:ubl:schema:xsd:TransportExecutionPlan-2"
                            xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
                            xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2">      
		   <cac:Consignment>
		   <!--Gross weight specified and specified as the  on THU level -->      
			  <cbc:GrossWeightMeasure unitCode="KGM">2030</cbc:GrossWeightMeasure>
			  <cac:TransportHandlingUnit>
				<cbc:ID>11111222222222</cbc:ID>
				<cac:MeasurementDimension>
					<cbc:AttributeID>AAB</cbc:AttributeID>
					<cbc:Measure unitCode="KGM">1030</cbc:Measure>      
				 </cac:MeasurementDimension>         
			  </cac:TransportHandlingUnit>
			  <cac:TransportHandlingUnit>
				<cbc:ID>11111222222223</cbc:ID>
				<cac:MeasurementDimension>
					<cbc:AttributeID>AAB</cbc:AttributeID>
					<cbc:Measure unitCode="KGM">1030</cbc:Measure>      
				 </cac:MeasurementDimension>         
			  </cac:TransportHandlingUnit>
		   </cac:Consignment>
		</ubl:TransportExecutionPlan>
	</test>
</testSet>
