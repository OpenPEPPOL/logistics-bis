                            xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2">      
		   <cac:Consignment>      
			  <cbc:TotalTransportHandlingUnitQuantity unitCode="EA">2</cbc:TotalTransportHandlingUnitQuantity>
			  <cac:TransportHandlingUnit>
				<cbc:ID>11111222222222</cbc:ID>         
			  </cac:TransportHandlingUnit>
			  <cac:TransportHandlingUnit>
				<cbc:ID>111112222222223</cbc:ID>         
			  </cac:TransportHandlingUnit> 
		   </cac:Consignment>
		</ubl:TransportExecutionPlan>
	</test>
	
	<test>
		<assert>
			<warning>PEPPOL-T124-R004:b</warning>
		</assert>
		<ubl:TransportExecutionPlan xmlns:ubl="urn:oasis:names:specification:ubl:schema:xsd:TransportExecutionPlan-2"
                            xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
                            xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2">      
		   <cac:Consignment>      
			  <cbc:TotalTransportHandlingUnitQuantity unitCode="EA">2</cbc:TotalTransportHandlingUnitQuantity>
			  <cac:TransportHandlingUnit>
				<cbc:ID>11111222222222</cbc:ID>         
			  </cac:TransportHandlingUnit>			   
		   </cac:Consignment>
		</ubl:TransportExecutionPlan>
	</test>
	
	<test>
		<assert>
			<success>PEPPOL-T124-R004:c</success>
		</assert>
		<ubl:TransportExecutionPlan xmlns:ubl="urn:oasis:names:specification:ubl:schema:xsd:TransportExecutionPlan-2"
                            xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
                            xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2">      
		   <cac:Consignment>      
			 			  			   
		   </cac:Consignment>
		</ubl:TransportExecutionPlan>
	</test>
	
	<test>
		<assert>
			<warning>PEPPOL-T124-R004:e</warning>
		</assert>
		<ubl:TransportExecutionPlan xmlns:ubl="urn:oasis:names:specification:ubl:schema:xsd:TransportExecutionPlan-2"
                            xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
                            xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2">      
		   <cac:Consignment>      
			  <assert>
				<warning>PEPPOL-T120-R004</warning>
			  </assert>
			  <cac:TransportHandlingUnit>
				<cbc:ID>11111222222222</cbc:ID>         
			  </cac:TransportHandlingUnit>
			  <cac:TransportHandlingUnit>
				<cbc:ID>111112222222223</cbc:ID>         
			  </cac:TransportHandlingUnit> 
		   </cac:Consignment>
		</ubl:TransportExecutionPlan>
