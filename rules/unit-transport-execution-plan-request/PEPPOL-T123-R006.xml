<?xml version="1.0" encoding="UTF-8"?>
<testSet xmlns="http://difi.no/xsd/vefa/validator/1.0"
	configuration="peppolbis-T123-base-1.0">
	<assert>
		<description>Verify that the grossvolume on consignment match the sum of the grossvolume on the THU if they are specified</description>
		<scope>PEPPOL-T123-R006</scope>
	</assert>
	<test id="1">
		<assert>
			<success>PEPPOL-T123-R006</success>
		</assert>
		<ubl:TransportExecutionPlanRequest xmlns:ubl="urn:oasis:names:specification:ubl:schema:xsd:TransportExecutionPlanRequest-2"
			xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
			xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2">
			<cac:Consignment>
		   <!--Gross volume specified and specified as the  on THU level -->      
			  <cbc:GrossVolumeMeasure unitCode="MTQ">1.030</cbc:GrossVolumeMeasure>
			  <cac:TransportHandlingUnit>
				<cbc:ID>11111222222222</cbc:ID>
				<cac:MeasurementDimension>
					<cbc:AttributeID>AAW</cbc:AttributeID>
					<cbc:Measure unitCode="MTQ">1.030</cbc:Measure>      
				 </cac:MeasurementDimension>         
			  </cac:TransportHandlingUnit>
		   </cac:Consignment>
		</ubl:TransportExecutionPlanRequest>
	</test>
	
	<test id="2">
		<assert>
			<success>PEPPOL-T123-R006</success>
		</assert>
		<ubl:TransportExecutionPlanRequest xmlns:ubl="urn:oasis:names:specification:ubl:schema:xsd:TransportExecutionPlanRequest-2"
			xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
			xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2">
			<cac:Consignment>
		   <!--Gross volume specified and not specified on THU level -->      
			  <cbc:GrossVolumeMeasure unitCode="MTQ">1.030</cbc:GrossVolumeMeasure>
			  <cac:TransportHandlingUnit>
				<cbc:ID>11111222222222</cbc:ID>				      
			  </cac:TransportHandlingUnit>
		   </cac:Consignment>
		</ubl:TransportExecutionPlanRequest>
	</test>
	
	<test id="3">
		<assert>
			<success>PEPPOL-T123-R006</success>
		</assert>
		<ubl:TransportExecutionPlanRequest xmlns:ubl="urn:oasis:names:specification:ubl:schema:xsd:TransportExecutionPlanRequest-2"
			xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
			xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2">
			<cac:Consignment>
					<!--Gross volume specified and specified as the  on THU level -->
					<cbc:GrossVolumeMeasure unitCode="MTQ">2.030</cbc:GrossVolumeMeasure>
					<cac:TransportHandlingUnit>
						<cbc:ID>11111222222222</cbc:ID>
						<cac:MeasurementDimension>
							<cbc:AttributeID>AAW</cbc:AttributeID>
							<cbc:Measure unitCode="MTQ">1.030</cbc:Measure>
						</cac:MeasurementDimension>
					</cac:TransportHandlingUnit>
					<cac:TransportHandlingUnit>
						<cbc:ID>11111222222223</cbc:ID>
						<cac:MeasurementDimension>
							<cbc:AttributeID>AAW</cbc:AttributeID>
							<cbc:Measure unitCode="MTQ">1.000</cbc:Measure>
						</cac:MeasurementDimension>
					</cac:TransportHandlingUnit>
				</cac:Consignment>
			</ubl:TransportExecutionPlanRequest>
	</test>

	<test id="4">
		<assert>
			<success>PEPPOL-T123-R006</success>
		</assert>
		<ubl:TransportExecutionPlanRequest xmlns:ubl="urn:oasis:names:specification:ubl:schema:xsd:TransportExecutionPlanRequest-2"
			xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
			xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2">
			<cac:Consignment>
				<!--Gross volume greater than specified  on THU level -->
				<cbc:GrossVolumeMeasure unitCode="MTQ">2.060</cbc:GrossVolumeMeasure>
				<cac:TransportHandlingUnit>
					<cbc:ID>11111222222222</cbc:ID>
					<cac:MeasurementDimension>
						<cbc:AttributeID>AAW</cbc:AttributeID>
						<cbc:Measure unitCode="MTQ">1.030</cbc:Measure>
					</cac:MeasurementDimension>
				</cac:TransportHandlingUnit>
				<cac:TransportHandlingUnit>
					<cbc:ID>11111222222223</cbc:ID>
					<cac:MeasurementDimension>
						<cbc:AttributeID>AAW</cbc:AttributeID>
						<cbc:Measure unitCode="MTQ">1.000</cbc:Measure>
					</cac:MeasurementDimension>
				</cac:TransportHandlingUnit>
			</cac:Consignment>
		</ubl:TransportExecutionPlanRequest>
	</test>
	
	<test id="5">
		<assert>
			<success>PEPPOL-T123-R006</success>
		</assert>
		<ubl:TransportExecutionPlanRequest xmlns:ubl="urn:oasis:names:specification:ubl:schema:xsd:TransportExecutionPlanRequest-2"
			xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
			xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2">
			<cac:Consignment>
				<!--Gross volume not specified, THU level is -->
				<cac:TransportHandlingUnit>
					<cbc:ID>11111222222222</cbc:ID>
					<cac:MeasurementDimension>
						<cbc:AttributeID>AAW</cbc:AttributeID>
						<cbc:Measure unitCode="MTQ">1.030</cbc:Measure>
					</cac:MeasurementDimension>
				</cac:TransportHandlingUnit>
			</cac:Consignment>
		</ubl:TransportExecutionPlanRequest>
	</test>

	<test id="6">
		<assert>
			<success>PEPPOL-T123-R006</success>
		</assert>
		<ubl:TransportExecutionPlanRequest xmlns:ubl="urn:oasis:names:specification:ubl:schema:xsd:TransportExecutionPlanRequest-2"
			xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
			xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2">
			<cac:Consignment>
				<!--Gross volume specified, THU level is not -->
				<cbc:GrossVolumeMeasure unitCode="MTQ">1.030</cbc:GrossVolumeMeasure>
			</cac:Consignment>
		</ubl:TransportExecutionPlanRequest>
	</test>
	
	<test id="7">
		<assert>
			<warning>PEPPOL-T123-R006</warning>
		</assert>
		<ubl:TransportExecutionPlanRequest xmlns:ubl="urn:oasis:names:specification:ubl:schema:xsd:TransportExecutionPlanRequest-2"
			xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
			xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2">
			<cac:Consignment>
		   <!--Gross weight specified and specified as the  on THU level -->      
			  <cbc:GrossVolumeMeasure unitCode="MTQ">2.030</cbc:GrossVolumeMeasure>
			  <cac:TransportHandlingUnit>
				<cbc:ID>11111222222222</cbc:ID>
				<cac:MeasurementDimension>
					<cbc:AttributeID>AAW</cbc:AttributeID>
					<cbc:Measure unitCode="MTQ">1.030</cbc:Measure>      
				 </cac:MeasurementDimension>         
			  </cac:TransportHandlingUnit>
			  <cac:TransportHandlingUnit>
				<cbc:ID>11111222222223</cbc:ID>
				<cac:MeasurementDimension>
					<cbc:AttributeID>AAW</cbc:AttributeID>
					<cbc:Measure unitCode="MTQ">1.030</cbc:Measure>      
				 </cac:MeasurementDimension>         
			  </cac:TransportHandlingUnit>
		   </cac:Consignment>
		</ubl:TransportExecutionPlanRequest>
	</test>
	<test id="8">
		<assert>
			<success>PEPPOL-T123-R006</success>
		</assert>
		<ubl:TransportExecutionPlanRequest xmlns:ubl="urn:oasis:names:specification:ubl:schema:xsd:TransportExecutionPlanRequest-2"
			xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
			xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2">
			<cac:Consignment>
				<!--Gross volume specified, THU level AAW is not -->
				<cbc:GrossVolumeMeasure unitCode="MTQ">2.030</cbc:GrossVolumeMeasure>
				<cac:TransportHandlingUnit>
					<cbc:ID>11111222222222</cbc:ID>
				</cac:TransportHandlingUnit>
			</cac:Consignment>
		</ubl:TransportExecutionPlanRequest>
	</test>
</testSet>
