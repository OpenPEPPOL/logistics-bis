<?xml version="1.0" encoding="UTF-8"?>
<testSet xmlns="http://difi.no/xsd/vefa/validator/1.0" configuration="peppolbis-T120-base-1.1">
	<assert>
		<description>Shipment  gross weight measure SHALL match the gross weight of the transport handling units specified</description>
		<scope>PEPPOL-T120-R018</scope>
	</assert>
	
	<test id="1">
		<assert>
			<success>PEPPOL-T120-R018</success>
		</assert>
